#include<stdio.h> 

int main(){

           int M,N,rem,i;

           printf("   Give the First number for the Range :   \n");

           scanf("%d",&M);

           printf("   Give the Final number for the Range :   \n");

           scanf("%d",&N);

           printf("\n The Even numbers between %d and %d are ",M,N); 

           for(i=M; i<=N; ++i){

               rem = i % 2;

               if(rem == 0)

               printf("\n  %d",i);

           }

    return 0; 

    } 
